{
  "name": "Mokka Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#B3B3B3",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "scope": [],
      "settings": {
        "foreground": "#1E1E1E"
      }
    },

    {
      "scope": [
        "comment",
        "punctuation",
        "meta.brace",
        "keyword.operator",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#666666"
      }
    },

    {
      "scope": [
        "variable",
        "meta.field.declaration",
        "meta.property-list",
        "text",
        "meta.objectliteral",
        "meta.jsx.children",
        "meta.type.annotation",
        "entity.name.tag.reference.scss"
      ],
      "settings": {
        "foreground": "#B3B3B3"
      }
    },

    {
      "scope": ["keyword", "storage", "keyword.operator.expression.typeof"],
      "settings": {
        "foreground": "#B97ACC"
      }
    },

    {
      "scope": [
        "entity.name.function",
        "variable.language.super",
        "support.function",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#73B2E6"
      }
    },

    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "entity.other.attribute-name.class.css",
        "meta.attribute-selector.scss",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#9CCC7A"
      }
    },

    {
      "scope": [
        "variable.language.this",
        "variable.other.readwrite.alias",
        "entity.name.tag",
        "variable.scss",
        "entity.name.tag.html",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#E66772"
      }
    },

    {
      "scope": [
        "constant.numeric",
        "meta.tag.attributes",
        "meta.property-value",
        "variable.parameter.url.scss",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#E6A467"
      }
    },

    {
      "scope": ["support", "meta.type.declaration", "entity.name.type"],
      "settings": {
        "foreground": "#E6BD73"
      }
    }
  ]
}
